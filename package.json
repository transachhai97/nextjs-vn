{
  "name": "nextjs-vn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docsify": "docsify serve docs --port 1234",
    "analyze": "cross-env ANALYZE=true yarn build && cat .next/stats.txt",
    "stylelint": "stylelint \"src/**/*.{js,jsx,htm,html,css,sss,less,scss,sass}\" --fix",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --fix",
    "clean": "yarn stylelint && yarn eslint",
    "dev": "yarn clean && next dev",
    "build": "yarn clean && next build",
    "start": "yarn clean && next start",
    "postbuild": "next-on-netlify",
    "deploy": "yarn build && netlify deploy --prod"
  },
  "dependencies": {
    "next": "9.5.3",
    "next-pwa": "^3.1.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@welldone-software/why-did-you-render": "^5.0.0-alpha.2",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-nextjs": "^1.0.6",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "globby": "^11.0.1",
    "next-compose-plugins": "^2.2.0",
    "next-on-netlify": "^2.4.0",
    "prettier": "^2.1.1",
    "stylelint": "^13.7.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "webpack-bundle-size-analyzer": "^3.1.0"
  }
}
